#: data/org.vanillaos.FirstSetup.desktop.in:3
msgid "Vanilla OS First Setup"
msgstr "Першая ўстаноўка Vanilla OS"

#: data/org.vanillaos.FirstSetup.appdata.xml.in:7
msgid "Tweak your Vanilla OS installation."
msgstr "Наладзьце ўстаноўку Vanilla OS."

#: data/org.vanillaos.FirstSetup.appdata.xml.in:10
msgid "No description"
msgstr "Без апісання"

#: data/org.vanillaos.FirstSetup.appdata.xml.in:26
msgid "First release"
msgstr "Першы выпуск"

#: vanilla_first_setup/gtk/default-conn-check.ui:12
#: vanilla_first_setup/defaults/conn_check.py:79
msgid "Checking Connection…"
msgstr "Праверка злучэння…"

#: vanilla_first_setup/gtk/default-conn-check.ui:13
#: vanilla_first_setup/defaults/conn_check.py:80
msgid "Please wait until the connection check is done."
msgstr "Калі ласка, пачакайце, пакуль скончыцца праверка злучэння."

#: vanilla_first_setup/gtk/default-hostname.ui:14
msgid "Device Details"
msgstr "Дэталі прылады"

#: vanilla_first_setup/gtk/default-hostname.ui:15
msgid "Provide details about your device"
msgstr "Дайце падрабязную інфармацыю аб вашай прыладзе"

#: vanilla_first_setup/gtk/default-hostname.ui:22
msgid "Device Name"
msgstr "Назва прылады"

#: vanilla_first_setup/gtk/default-hostname.ui:35
#: vanilla_first_setup/gtk/default-theme.ui:49
msgid "Next"
msgstr "Далей"

#: vanilla_first_setup/gtk/default-theme.ui:17
msgid "Default"
msgstr "Па змаўчанні"

#: vanilla_first_setup/gtk/default-theme.ui:29
msgid "Dark"
msgstr "Цёмны"

#: vanilla_first_setup/gtk/default-theme.ui:42
msgid "Color Scheme"
msgstr "Колеравая схема"

#: vanilla_first_setup/gtk/default-theme.ui:43
msgid "Choose a color scheme for your system."
msgstr "Выберыце каляровую схему для вашай сістэмы."

#: vanilla_first_setup/gtk/default-welcome.ui:12
msgid "Welcome!"
msgstr "Сардэчна запрашаем!"

#: vanilla_first_setup/gtk/default-welcome.ui:13
msgid "Make your choices, this wizard will take care of everything."
msgstr "Рабіце свой выбар, гэты майстар паклапоціцца пра ўсё."

#: vanilla_first_setup/gtk/dialog.ui:6
msgid "Showing Information"
msgstr "Паказ інфармацыі"

#: vanilla_first_setup/gtk/done.ui:12
msgid "All done!"
msgstr "Гатова!"

#: vanilla_first_setup/gtk/done.ui:13
msgid "Restart your device to enjoy your Vanilla OS experience."
msgstr "Перазагрузіце прыладу, каб атрымаць асалоду ад АС Vanilla."

#: vanilla_first_setup/gtk/layout-yes-no.ui:36
msgid "Yes, enable it"
msgstr "Так, уключыць"

#: vanilla_first_setup/gtk/post-script.ui:11
msgid "Finalizing"
msgstr "Дапрацоўка"

#: vanilla_first_setup/gtk/post-script.ui:12
msgid "Your device will be ready soon."
msgstr "Ваша прылада хутка будзе гатовая."

#: vanilla_first_setup/gtk/progress.ui:70
msgid "Installing..."
msgstr "Усталяванне..."

#: vanilla_first_setup/gtk/progress.ui:91
msgid "Show Console output"
msgstr "Паказаць выхад кансолі"

#: vanilla_first_setup/gtk/progress.ui:106
msgid "Show Slideshow"
msgstr "Паказаць слайд-шоў"

#: vanilla_first_setup/gtk/tour.ui:27
msgid "Read More"
msgstr "Чытаць далей"

#: vanilla_first_setup/gtk/window.ui:25
msgid "Back"
msgstr "Назад"

#: vanilla_first_setup/defaults/conn_check.py:70
msgid "No Internet Connection!"
msgstr "Няма падключэння да Інтэрнэту!"

#: vanilla_first_setup/defaults/conn_check.py:71
msgid "First Setup requires an active internet connection"
msgstr "Першая ўстаноўка патрабуе актыўнага інтэрнэт-злучэння"

#: vanilla_first_setup/layouts/preferences.py:75
msgid "Please select at least one option."
msgstr "Выберыце хаця б адзін варыянт."

#: vanilla_first_setup/views/done.py:41
msgid "Restart your device to enjoy your {} experience."
msgstr "Перазагрузіце прыладу, каб атрымаць асалоду ад {}."

#: vanilla_first_setup/views/done.py:63
msgid "Something went wrong"
msgstr "Нешта пайшло не так"

#: vanilla_first_setup/views/done.py:64
msgid "Please contact the distribution developers."
msgstr "Калі ласка, звяжыцеся з распрацоўшчыкамі дыстрыбутыва."

#: vanilla_first_setup/main.py:55
msgid "Run a post script"
msgstr "Запусціце пост-скрыпт"

#: vanilla_first_setup/main.py:63
msgid "Run as a new user"
msgstr "Запусціце як новы карыстальнік"

#: vanilla_first_setup/window.py:67
msgid "Done!"
msgstr "Гатова!"

#: vanilla_first_setup/window.py:67
msgid "Your device is ready to use."
msgstr "Ваша прылада гатова да выкарыстання."

#: vanilla_first_setup/window.py:68
msgid "Error!"
msgstr "Памылка!"

#: vanilla_first_setup/window.py:68
msgid "Something went wrong."
msgstr "Нешта пайшло не так."
