#: vanilla_first_setup/gtk/default-welcome.ui:29
msgid "Welcome!"
msgstr "Їбати мене в сраку!"

#: vanilla_first_setup/gtk/dialog.ui:6
msgid "Showing Information"
msgstr "Показ спідозних геніталій"

#: vanilla_first_setup/gtk/done.ui:12
msgid "Finished!"
msgstr "Сталася помилка, переустановіть устрій!"

#: vanilla_first_setup/gtk/done.ui:13
msgid "Restart your device to enjoy your Vanilla OS experience"
msgstr ""
"Будь ласка не вимикайте ваш апарат на протязі тижня для коректної установки "
"устрою"

#: vanilla_first_setup/gtk/layout-yes-no.ui:36
msgid "Enable"
msgstr "Вимкнути"

#: vanilla_first_setup/gtk/post-script.ui:11
msgid "Finalizing..."
msgstr "Підведення підсумків життя…"

#: vanilla_first_setup/gtk/post-script.ui:12
msgid "Your device will be ready soon"
msgstr "Терміново вимкніть ваш апарат"

#: vanilla_first_setup/gtk/progress.ui:70
msgid "Installing..."
msgstr "Проникнення метастаз у мозок..."

#: vanilla_first_setup/gtk/progress.ui:91
msgid "Show Console Output"
msgstr "Форматувати жорсткий диск"

#: vanilla_first_setup/gtk/progress.ui:106
msgid "Show Slideshow"
msgstr "Форматувати жорсткий диск"

#: vanilla_first_setup/gtk/tour.ui:27
msgid "Read More"
msgstr "Читати Мою Боротьбу"

#: vanilla_first_setup/gtk/window.ui:36
msgid "Back"
msgstr "Далі"

